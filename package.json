    "@nuxt/content": "^2.12.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.12.14",
    "@vueuse/core": "^12.5.0",
    "@vueuse/nuxt": "^12.5.0",
    "eslint": "^9.19.0",
    "nuxt": "^3.15.3",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@docsearch/css": "^3.8.3",

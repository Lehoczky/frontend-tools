{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "npm run eslint:check && npm run styles:check && npm run format:check",
    "eslint:check": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "styles:check": "stylelint \"./**/*.{css,vue}\" --ignore-path=.gitignore",
    "format:check": "prettier \"./**\" --check --ignore-unknown",
    "format:write": "prettier \"./**\" --write --ignore-unknown",
    "build": "nuxt build",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@lehoczky/eslint-config-vue": "^1.2.5",
    "@lehoczky/markdownlint-config": "^1.1.4",
    "@lehoczky/prettier-config": "^1.1.6",
    "@nuxt/content": "^2.12.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.12.12",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "eslint": "^8.57.0",
    "eslint-define-config": "^2.1.0",
    "nuxt": "^3.11.2",
    "postcss-html": "^1.7.0",
    "postcss-preset-env": "^9.5.13",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@docsearch/css": "^3.6.0",
    "@docsearch/js": "^3.6.0",
    "@nuxtjs/algolia": "^1.10.2",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "packageManager": "pnpm@9.1.1"
}

{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "npm run eslint:check && npm run styles:check && npm run format:check",
    "eslint:check": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "styles:check": "stylelint \"./**/*.{css,vue}\" --ignore-path=.gitignore",
    "format:check": "prettier \"./**\" --check --ignore-unknown",
    "format:write": "prettier \"./**\" --write --ignore-unknown",
    "build": "nuxt build",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@lehoczky/eslint-config-vue": "^0.0.22",
    "@lehoczky/markdownlint-config": "^0.0.2",
    "@lehoczky/prettier-config": "^1.0.1",
    "@nuxt/content": "^2.9.0",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@types/lodash-es": "^4.17.10",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "eslint": "^8.52.0",
    "eslint-define-config": "^1.24.1",
    "nuxt": "~3.8.0",
    "nuxt-simple-robots": "^3.1.9",
    "nuxt-simple-sitemap": "^3.4.1",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "@nuxtjs/algolia": "^1.10.1",
    "lodash-es": "^4.17.21"
  },
  "packageManager": "pnpm@8.10.0"
}

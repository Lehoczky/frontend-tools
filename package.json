{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "npm run eslint:check && npm run styles:check && npm run format:check",
    "eslint:check": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "styles:check": "stylelint \"./**/*.{css,vue}\" --ignore-path=.gitignore",
    "format:check": "prettier \"./**\" --check --ignore-unknown",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "gh:deploy": "gh-pages -d dist -t true"
  },
  "devDependencies": {
    "@lehoczky/eslint-config-vue": "^0.0.10",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^9.4.0",
    "@vueuse/nuxt": "^9.4.0",
    "eslint": "^8.27.0",
    "eslint-define-config": "^1.11.0",
    "gh-pages": "^4.0.0",
    "nuxt": "^3.0.0-rc.13",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.2.2",
    "tailwindcss-fluid-type": "^2.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "packageManager": "pnpm@7.14.2"
}

{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "npm run eslint:check && npm run styles:check && npm run format:check",
    "eslint:check": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "styles:check": "stylelint \"./**/*.{css,vue}\" --ignore-path=.gitignore",
    "format:check": "prettier \"./**\" --check --ignore-unknown",
    "format:write": "prettier \"./**\" --write --ignore-unknown",
    "build": "nuxt build",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@lehoczky/eslint-config-vue": "^1.2.1",
    "@lehoczky/markdownlint-config": "^1.1.1",
    "@lehoczky/prettier-config": "^1.1.2",
    "@nuxt/content": "^2.12.0",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.11.19",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "nuxt": "~3.10.2",
    "postcss-html": "^1.6.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "@nuxtjs/algolia": "^1.10.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5"
  },
  "packageManager": "pnpm@8.15.3"
}

{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "npm run eslint:check && npm run styles:check && npm run format:check",
    "eslint:check": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "styles:check": "stylelint \"./**/*.{css,vue}\" --ignore-path=.gitignore",
    "format:check": "prettier \"./**\" --check --ignore-unknown",
    "generate": "nuxt generate",
    "preview": "serve .output/public",
    "deploy": "gh-pages -d dist -t true"
  },
  "devDependencies": {
    "@lehoczky/eslint-config-vue": "^0.0.4",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^9.3.0",
    "@vueuse/nuxt": "^9.3.0",
    "eslint": "^8.15.0",
    "eslint-define-config": "^1.7.0",
    "gh-pages": "^4.0.0",
    "nuxt": "3.0.0-rc.11",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "serve": "^14.0.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.1.8",
    "tailwindcss-fluid-type": "^1.3.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}

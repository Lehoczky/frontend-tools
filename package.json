{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "npm run eslint:check && npm run styles:check && npm run format:check",
    "eslint:check": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "styles:check": "stylelint \"./**/*.{css,vue}\" --ignore-path=.gitignore",
    "format:check": "prettier \"./**\" --check --ignore-unknown",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "gh:deploy": "npm run generate && gh-pages -d dist -t true"
  },
  "devDependencies": {
    "@lehoczky/eslint-config-vue": "^0.0.14",
    "@nuxt-themes/elements": "^0.9.4",
    "@nuxt-themes/typography": "^0.11.0",
    "@nuxt/content": "^2.6.0",
    "@types/lodash-es": "^4.17.7",
    "@types/prettier": "^2.7.2",
    "@vueuse/core": "^10.0.2",
    "@vueuse/nuxt": "^10.0.2",
    "eslint": "^8.38.0",
    "eslint-define-config": "^1.18.0",
    "gh-pages": "^5.0.0",
    "nuxt": "^3.4.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "stylelint": "^15.5.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "packageManager": "pnpm@8.3.1"
}
